}

.Answers {
    width: 80%;
    font-size: 16px;
    -unity-font-style: bold;
    white-space: normal;
    color: rgb(0, 0, 0);
}

.QuestionContainer {
    height: 20%;
    width: 100%;
}

.Question {
    font-size: 16px;
    -unity-font-style: bold;
    color: rgb(0, 0, 0);
    height: auto;
    white-space: normal;
}
